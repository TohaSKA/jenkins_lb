@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.shopCart.listShopCarItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Masina:</b> @el.car.name<br />
            <b>Pretul:</b> @el.car.price.ToString("c")

        </div>
    }
    <hr />
    <a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Plateste</a>
</div>